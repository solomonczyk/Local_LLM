# Пример файла переменных окружения
# Скопируйте в .env и заполните реальными значениями

# КРИТИЧЕСКИ ВАЖНО: Никогда не коммитьте реальные ключи в Git!

# API ключи (ОБЯЗАТЕЛЬНО)
AGENT_API_KEY=your_secure_api_key_here_minimum_32_characters
LLM_API_KEY=your_llm_api_key_here

# OpenAI Director (для гибридной архитектуры)
OPENAI_API_KEY=your_openai_key_here
SHADOW_DIRECTOR_ENABLED=true
DIRECTOR_MODEL=gpt-4o-mini

# База данных
DATABASE_URL=postgresql://user:password@localhost:5432/agent_db
POSTGRES_USER=agent_user
POSTGRES_PASSWORD=secure_password_here_minimum_16_characters
POSTGRES_DB=agent_memory

# Безопасность
SECRET_KEY=your_secret_key_here_minimum_32_characters
JWT_SECRET=your_jwt_secret_here_minimum_32_characters

# Сервисы
LLM_SERVER_URL=http://localhost:8002/v1
TOOL_SERVER_URL=http://localhost:8003
UI_SERVER_URL=http://localhost:7865

# Рабочая директория
WORKSPACE_ROOT=/path/to/workspace

