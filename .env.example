# Пример файла переменных окружения
# Скопируйте в .env и заполните реальными значениями

# API ключи
AGENT_API_KEY=your_secure_api_key_here
LLM_API_KEY=your_llm_api_key_here

# База данных
DATABASE_URL=postgresql://user:password@localhost:5432/agent_db
POSTGRES_USER=agent_user
POSTGRES_PASSWORD=secure_password_here
POSTGRES_DB=agent_memory

# Безопасность
SECRET_KEY=your_secret_key_here
JWT_SECRET=your_jwt_secret_here

# Сервисы
LLM_SERVER_URL=http://localhost:8002/v1
TOOL_SERVER_URL=http://localhost:8003
UI_SERVER_URL=http://localhost:7865

# Рабочая директория
WORKSPACE_ROOT=/path/to/workspace